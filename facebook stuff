#fb-button {
  background-image: url("http://i63.tinypic.com/u4fx4.png");
  border:0;
  display:block;
  width:200px;
  height:35px;
  margin-top:20px;
  margin-bottom:20px;



  $('#fb-button').on('click', function(e){
    e.preventDefault();
    console.log("test")
    $.auth.oAuthSignIn({provider: 'facebook'});
  })
