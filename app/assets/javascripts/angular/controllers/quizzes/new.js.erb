app.controller('QuizzesNewCtrl', ['$scope', '$http', function($scope, $http){

  $scope.quiz = {};
  $scope.form = {};

  $scope.form.create = function(){
    var data = {
      post: $scope.quiz
    };

    $http.post('/quizzes', data).success(function(response){
      console.log(response);
    });
  };
}]);