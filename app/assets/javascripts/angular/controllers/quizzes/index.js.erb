app.controller('QuizzesIndexCtrl', ['$scope', '$http', '$location', function($scope, $http, $location){
  $http.get('/quizzes').success(function(response){
    console.log(response);
    $scope.quizzes = response;
  });

  $scope.delete = function (id) {
    $http.delete('/quizzes/' + id).success(function(response){
      location = '/#/quizzes'
    });
  };
}]);